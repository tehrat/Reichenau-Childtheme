/* Facebook Feed */

.fb-feed  {
    margin: 36px -24px 0 -24px;
    zoom: 1;
}

.fb-feed .fb-feed-script  {
    display: none;
}

.fb-feed h3.widget-title {
    margin-left: 24px;
}

.fb-feed h3.widget-title a {
    color: #fff;
}

.fb-feed h3.widget-title:hover {
    background-color: #ec6c49;
    transition: background-color 0.2s ease-in;
}

.fb-feed .fb-feed-figure {
    padding: 0 24px;
    width: 24%;
    margin-bottom: 48px;
    box-sizing: border-box;
    display: -moz-inline-stack;
    display: inline-block;
    vertical-align: top;
}

.fb-feed .fb-feed-figure img {
    width: 33.33%;
    float: left;
    margin-right: 24px;
    box-sizing: border-box;
}

.fb-feed .fb-feed-figcaption {
    margin-top: -5px;
    color: #5e5c58;
    font-size: 18px; 
}

.fb-feed .fb-feed-figcaption {
    margin-top: -5px;
    color: #5e5c58;
    font-size: 18px; 
}

.fb-feed a .fb-feed-figcaption {
    transition: color 0.4s ease-out;
}

.fb-feed a:hover .fb-feed-figcaption {
    color: #ec6c49;
    transition: color 0.2s ease-in;
}

.fb-feed .fb-feed-figcaption .fb-feed-date {
    font-family: 'Futura-Book', Georgia, serif;
    color: #221F16;
    display: block;
}

@media (max-width: 1024px) {
    
    .fb-feed .fb-feed-figure {
        width: 32.333%;
    }  

}

@media (max-width: 680px) {
    
    .fb-feed .fb-feed-figure {
        width: 49%;
    }  

}

@media (max-width: 480px) {
    
    .fb-feed .fb-feed-figure {
        width: 99%;
    }  

}

